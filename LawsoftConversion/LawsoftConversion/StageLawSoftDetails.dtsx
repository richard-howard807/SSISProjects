                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT CAST(AllData.LSMatter AS NVARCHAR(255)) AS FEDCode,
       AllData.[Detail Desc],
       CAST(AllData.CaseText AS NVARCHAR(255)) AS CaseText,
       AllData.CaseDate,
       AllData.CaseValue,
       AllData.[Lookup code / Datalist table],
       AllData.[Lookup Table],
       AllData.[MS Code],
       AllData.[MS Table],
       AllData.[Data Type]
	   FROM
(
SELECT LSMatter,
       CAST(LawsoftDetailMapping.LSFieldName AS NVARCHAR(255)) AS [Detail Desc],
       
	   NULL  AS CaseText,
	   NULL AS CaseDate,
       CaseValue AS CaseValue,
       txtLookupCode AS [Lookup code / Datalist table],
       txtLookupTable AS [Lookup Table],
       txtMSCode AS [MS Code],
       txtMSTable AS [MS Table],	
	   [Data Type] AS [Data Type]	

FROM  LawsoftStaging.dbo.LSCaseValue
 LEFT OUTER JOIN dbo.LawsoftDetailMapping
  ON LawsoftDetailMapping.LSFieldName = LSCaseValue.LSFieldName
UNION
SELECT LSMatter,
       CAST(LawsoftDetailMapping.LSFieldName AS NVARCHAR(255)) AS [Detail Desc],
       
	   NULL  AS CaseText,
	   CaseDate AS CaseDate,
       NULL AS CaseValue,
       txtLookupCode AS [Lookup code / Datalist table],
       txtLookupTable AS [Lookup Table],
       txtMSCode AS [MS Code],
       txtMSTable AS [MS Table],	
	   [Data Type] AS [Data Type]	

FROM  LawsoftStaging.dbo.LSCaseDate
 LEFT OUTER JOIN dbo.LawsoftDetailMapping
  ON LawsoftDetailMapping.LSFieldName = LSCaseDate.LSFieldName
UNION
SELECT LSMatter,
       CAST(LawsoftDetailMapping.LSFieldName AS NVARCHAR(255)) AS [Detail Desc],
       
	   CaseText  AS CaseText,
	   NULL AS CaseDate,
       NULL AS CaseValue,
       txtLookupCode AS [Lookup code / Datalist table],
       txtLookupTable AS [Lookup Table],
       txtMSCode AS [MS Code],
       txtMSTable AS [MS Table],	
	   [Data Type] AS [Data Type]	

FROM  LawsoftStaging.dbo.LSCaseText
 LEFT OUTER JOIN dbo.LawsoftDetailMapping
  ON LawsoftDetailMapping.LSFieldName = LSCaseText.LSFieldName

  ) AS AllData</property>
