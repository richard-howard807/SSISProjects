      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{20CFC438-7EEE-40D7-87C3-693A01676CCE}"
          SQLTask:SqlStatementSource="MERGE.dbo.udCRCore DEST&#xA;USING&#xA;(&#xA;   SELECT MS.fileID,&#xA;       CAST(txtCurenStatNot AS NVARCHAR(MAX)) AS [txtCurenStatNot]&#xA;   FROM MattersphereStaging.[dbo].[FWNotesUnstructured]&#xA;   INNER JOIN MattersphereStaging.dbo.FWMatterCreationStage&#xA;    ON [FWNotesUnstructured].SourceSystemID=FWMatterCreationStage.SourceSystemID&#xA;   INNER JOIN (SELECT clNo,fileno,fileID FROM config.dbFile INNER JOIN config.dbClient ON dbClient.clID = dbFile.clID) AS MS&#xA;    ON MS.clNo = FWMatterCreationStage.clNo&#xA;    AND MS.fileNo = CAST(FWMatterCreationStage.NewMatterNumber AS NVARCHAR(MAX))&#xA; WHERE CAST(txtCurenStatNot AS NVARCHAR(MAX)) &lt;&gt;''&#xA;) AS src&#xA;ON DEST.fileID = src.fileID&#xA;WHEN MATCHED THEN&#xA;    UPDATE SET DEST.[txtCurenStatNot] = src.[txtCurenStatNot]&#xA;WHEN NOT MATCHED THEN&#xA;    INSERT&#xA;    (&#xA;        fileID,&#xA;        [txtCurenStatNot]&#xA;    )&#xA;    VALUES&#xA;    (fileID, src.[txtCurenStatNot]);" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
