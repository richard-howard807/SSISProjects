                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select * from (SELECT MatterManager.usrAlias AS [Matter Owner],
CAST(BCMUser.usrAlias AS NVARCHAR(15))  AS TM,
CAST(td.Section  AS NVARCHAR(15)) AS Section,
CAST(s.SectionGroup  AS NVARCHAR(15)) AS SectionGroup,
CAST(td.Department AS NVARCHAR(15)) AS Department,
dbBranch.brCode AS Office
from dbFeeEarner fe
INNER JOIN dbo.dbUser AS MatterManager
 ON fe.feeusrID = MatterManager.usrID
INNER JOIN dbo.Syn3ETimeKeeper  t on fe.feeExtID = t.tkprindex
Inner Join dbo.Syn3ETkprDate  TD	on fe.feeExtID = TD.timekeeperlkup
Inner Join dbo.Syn3EnxUser  U		on U.NxUserID = T.TRE_User
Inner Join dbo.Syn3EnxFWKUser NxFWKUser 	on NxFWKUser.NxFWKUserID = U.NxUserID
INNER JOIN dbo.Syn3ESection  s	on s.code = td.Section
INNER JOIN dbUser dbu on  fe.[feeusrID] = dbu.usrID

-------SD 20160321 added this join to get the actual login for the users as the returned SupTkpr is in fact the index of the table and not the actual login
LEFT JOIN dbo.Syn3ETimeKeeper  as BCM on TD.SupTkpr  = BCM.[TkprIndex] 
LEFT JOIN dbUser as BCMUser on BCMUser.usrAlias collate database_default = BCM.Number collate database_default
LEFT JOIN dbFeeEarner as BCMFeeEarner on BCMFeeEarner.[feeusrID] = BCMUser.usrID
LEFT JOIN udDepartmentSectionLookup as DepartmentLookup on td.Section collate database_default = DepartmentLookup.section collate database_default and s.SectionGroup collate database_default = DepartmentLookup.sectiongroup collate database_default
LEFT JOIN udDepartment as Department on DepartmentLookup.departmentcode = Department.[code]
LEFT JOIN dbo.dbBranch ON td.Office COLLATE DATABASE_DEFAULT = dbBranch.brCode COLLATE DATABASE_DEFAULT


Where getDate() BETWEEN (NxStartDate ) AND (NxEndDate)
and t.tre_user is not null
and t.tkprstatus = 'Active'
AND MatterManager.usrActive=1

ORDER BY MatterManager.usrAlias) [refTable]
where [refTable].[Matter Owner] = ?</property>
