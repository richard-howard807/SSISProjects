            <component
              refId="Package\DFT - Get Lawsoft Involvement data\SRC_Lawsoft"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="SRC_Lawsoft"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT CAST(LSMatter AS NVARCHAR(255)) AS FEDCode
,'xxxENxxx' AS [client] 
,'xxxENxxx' AS [matter] 
,ROW_NUMBER() OVER (PARTITION BY LSMatter ORDER BY DIWOR) AS seq_no
,ENTRY_TYPE_ID AS [EntityType]
,NAME_ID AS [entity_code]
,ADDRESS_ID AS EnvisionAddID
,CAST(CASE WHEN ENTRY_TYPE_ID='WAYLEA'  THEN WayLeave.WAYLEAVE_REF 
WHEN ENTRY_TYPE_ID='PURSOL' THEN purch_sol_ref
WHEN ENTRY_TYPE_ID='WAYLEA' THEN wayleave_ref
WHEN ENTRY_TYPE_ID='OSSOL' THEN  grantsol_ref
WHEN ENTRY_TYPE_ID='GRASOL' THEN grantsol_ref
WHEN ENTRY_TYPE_ID='LESSOL' THEN grantsol_ref
WHEN ENTRY_TYPE_ID='TRASOL' THEN grantsol_ref
WHEN ENTRY_TYPE_ID='OWNSOL' THEN grantsol_ref
ELSE NULL END AS NVARCHAR(50)) AS InvolvementRef
,0 as UseDX
,DIWOR AS [AssocSource]
FROM Lawsoft.dbo.COMM_ENTRY
INNER JOIN [dbo].[EnvisionAssociatesLookup]
 ON ENTRY_TYPE_ID=EntityType COLLATE DATABASE_DEFAULT
INNER JOIN Lawsoft.dbo.MATTER
ON MATTER_DIWOR=LINKED_TABLE_ID
INNER JOIN dbo.LSToMSMatters
ON Lawsoft.dbo.MATTER.MATTER_DIWOR=SourceSystemID
LEFT OUTER JOIN Lawsoft.dbo.CONTACT
 ON NAME_ID=NAME_DIWOR
LEFT OUTER JOIN (SELECT MATTER_DIWOR,WAYLEAVE_REF FROM Lawsoft.dbo.DEED_OF_GRANT WHERE ISNULL(WAYLEAVE_REF,'')&lt;&gt;'') WayLeave
 ON WayLeave.MATTER_DIWOR = MATTER.MATTER_DIWOR
LEFT OUTER JOIN (SELECT MATTER_DIWOR,grantsol_ref FROM Lawsoft.dbo.DEED_OF_GRANT WHERE ISNULL(grantsol_ref,'')&lt;&gt;'') grantsol_ref
 ON grantsol_ref.MATTER_DIWOR = MATTER.MATTER_DIWOR
 LEFT OUTER JOIN (SELECT MATTER_DIWOR,purch_sol_ref FROM Lawsoft.dbo.Plot_sale_info WHERE ISNULL(purch_sol_ref,'')&lt;&gt;'') purch_sol_ref
 ON purch_sol_ref.MATTER_DIWOR = MATTER.MATTER_DIWOR

WHERE TABLE_ID='M'
AND ENTRY_TYPE_ID IS NOT NULL
AND NAME_ID &lt;&gt;0</property>
