                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--Get the FED Matters
--Strip leading 0s on the client where possible
;WITH CTEZeroStripper AS
(
SELECT 
CASE WHEN ISNUMERIC(caclient.cl_accode)=1 THEN 
CAST(caclient.cl_accode AS INT) ELSE caclient.cl_accode END AS ClientStripped,
caclient.cl_accode AS Client,
cashdr.matter AS Matter,
caclient.cl_clname AS ClientName,
cashdr.case_public_desc1 AS CaseDescription
FROM axxia01.dbo.cashdr cashdr
INNER JOIN axxia01.dbo.caclient caclient  ON cashdr.client = caclient.cl_accode 
WHERE ISNUMERIC(cl_accode)=1
AND cashdr.date_closed IS NULL
)
--Get the matters where the client can't have it's leading 0s stripped
,CTEUnstrippableClients
AS
(
SELECT 
cl_accode  AS ClientStripped,
caclient.cl_accode AS Client,
cashdr.matter AS Matter,
caclient.cl_clname AS ClientName,
cashdr.case_public_desc1 AS CaseDescription
FROM axxia01.dbo.cashdr cashdr
INNER JOIN axxia01.dbo.caclient caclient ON cashdr.client = caclient.cl_accode 
WHERE 
1=1
AND ISNUMERIC(cl_accode)=0
AND cashdr.date_closed IS NULL
)
,
--Union all of the stripped and non stripped client numbers together.
CTEUNION1 AS
(
SELECT
CAST(CTEZeroStripper.ClientStripped AS VARCHAR(8)) COLLATE DATABASE_DEFAULT AS ClientStripped ,
CTEZeroStripper.Client ,
CTEZeroStripper.Matter COLLATE DATABASE_DEFAULT AS  Matter,
RTRIM(CTEZeroStripper.ClientName) COLLATE DATABASE_DEFAULT AS ClientName,
RTRIM(CTEZeroStripper.CaseDescription) COLLATE DATABASE_DEFAULT AS CaseDescription
FROM CTEZeroStripper
WHERE CTEZeroStripper.Matter NOT IN ('ML','.','0')
UNION 
SELECT
RTRIM(CTEUnstrippableClients.ClientStripped) AS ClientStripped,
CTEUnstrippableClients.Client ,
CTEUnstrippableClients.Matter COLLATE DATABASE_DEFAULT AS  Matter,
RTRIM(CTEUnstrippableClients.ClientName) COLLATE DATABASE_DEFAULT AS ClientName,
RTRIM(CTEUnstrippableClients.CaseDescription) COLLATE DATABASE_DEFAULT AS CaseDescription
FROM CTEUnstrippableClients 
WHERE CTEUnstrippableClients.Matter NOT IN ('ML','.','0')
)
,CTEUNION2
AS
--Strip leading 0s on the matter where possible
(
SELECT
CTEUNION1.ClientStripped ,
CTEUNION1.Client ,
CAST(CTEUNION1.Matter AS INT) AS Matter,
CTEUNION1.ClientName ,
CTEUNION1.CaseDescription
FROM CTEUNION1
WHERE ISNUMERIC(CTEUNION1.Matter)=1
)
,CTEUNION3
--Get the matters where the matters can't have it's leading 0s stripped
AS
(
SELECT
CTEUNION1.ClientStripped ,
CTEUNION1.Client ,
CTEUNION1.Matter AS Matter,
CTEUNION1.ClientName ,
CTEUNION1.CaseDescription
FROM CTEUNION1
WHERE ISNUMERIC(CTEUNION1.Matter)=0
),CTEUNION 
AS
--Union all of the stripped and non stripped matter numbers together.
(
SELECT CTEUNION3.ClientStripped ,
       CTEUNION3.Client ,
       CTEUNION3.Matter ,
       CTEUNION3.ClientName ,
       CTEUNION3.CaseDescription
FROM CTEUnion3
UNION
SELECT CTEUNION2.ClientStripped ,
       CTEUNION2.Client ,
       CAST(CTEUNION2.Matter AS CHAR(8)) AS Matter,
       CTEUNION2.ClientName ,
       CTEUNION2.CaseDescription 
FROM CTEUNION2
)
SELECT
CONCAT(CTEUNION.ClientStripped,'-',RTRIM(CTEUNION.Matter),'=',CTEUNION.ClientName,'-',CTEUNION.CaseDescription)  AS OutputColumn
FROM CTEUNION                   
WHERE 
1=1
AND RTRIM(CTEUNION.Matter) &lt;&gt; '0'
UNION ALL
--Get the MatterSphereMatters
SELECT
CONCAT(cli.clNo,'-',fil.fileNo,'=',RTRIM(cli.clName),'-',fil.fileDesc) AS OutputColumn
FROM MS_Prod.dbo.udExtFile ext
INNER JOIN MS_Prod.config.dbFile fil ON fil.fileID = ext.fileID
INNER JOIN MS_Prod.config.dbClient cli ON fil.clID = cli.clID
WHERE ext.bitMSOnlyMM = 1 --Only the ones that are in Mattersphere.
AND fil.fileClosed IS NULL --Only open ones
AND fil.fileNo &lt;&gt; '0'
ORDER BY 1</property>
