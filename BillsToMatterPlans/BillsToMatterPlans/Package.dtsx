      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{8B057DE5-09AE-4119-A56A-DA627131E28A}"
          SQLTask:SqlStatementSource="DECLARE @Directory AS NVARCHAR(MAX)&#xA;SET @Directory='\\svr-liv-3efn-02\TE_3E_Share\TE_3E_PROD29\Inetpub\Attachment\InvMaster\' --LIVE&#xA;&#xA;&#xA;DECLARE @test AS INT&#xA;SET @test=0&#xA;&#xA;TRUNCATE TABLE dbo.BillsIntoMatterPlans&#xA;INSERT INTO dbo.BillsIntoMatterPlans&#xA;(&#xA;fileID&#xA;,case_id&#xA;,[Client Code]&#xA;,[Matter Number]&#xA;,[3e References]&#xA;,[Matter Description]&#xA;,[Date Opened]&#xA;,[Fee Earner]&#xA;,[Fee Earner Code]&#xA;,[Bill Number]&#xA;,[Bill Date]&#xA;,[Bill Amount]&#xA;,[Invoice Link]&#xA;,[Document Name]&#xA;,[SystemsDestination]&#xA;,[Is Credit Note]&#xA;,[Fee Earner Email]&#xA;,OriginatorName&#xA;,OriginatorEmail&#xA;,[ProformaComments]&#xA;,[BillingInstructions]&#xA;,[EmailType]&#xA;,[OriginatorEmailsNeeded]&#xA;)&#xA;&#xA;SELECT    &#xA; fileID&#xA;,NULL AS case_id&#xA;,[Client Code]&#xA;,[Matter Number]&#xA;,[3e References]&#xA;,[Matter Description]&#xA;,[Date Opened]&#xA;,[Fee Earner]&#xA;,[Fee Earner Code]&#xA;,[Bill Number]&#xA;,[Bill Date]&#xA;,[Bill Amount]&#xA;,[Invoice Link] + [FileName] AS [Invoice Link]&#xA;,'Invoice ' + CAST([Bill Number] AS NVARCHAR(30)) AS [Document Name]&#xA;,[SystemsDestination]&#xA;,[Is Credit Note]&#xA;,[Fee Earner Email]&#xA;,ISNULL(BaseUserName,'Helen Duffy') AS OriginatorName&#xA;,ISNULL(EmailAddr,'Helen.Duffy@weightmans.com') AS OriginatorEmail&#xA;,[ProformaComments]&#xA;,[BillingInstructions]&#xA;,[EmailType]&#xA;,CASE WHEN BaseUserName = [Fee Earner] collate database_default THEN  0 ELSE 1 END  AS [OriginatorEmailsNeeded]&#xA;&#xA; FROM&#xA; (&#xA;SELECT &#xA;dbfile.fileID&#xA;,CASE WHEN FEDCode is null THEN (CASE WHEN ISNUMERIC(clno)=1 THEN  RIGHT('00000000' + CONVERT(VARCHAR,clno), 8) ELSE CAST(RTRIM(clNo)  AS VARCHAR(8)) END) ELSE (CAST(SUBSTRING(RTRIM(FEDCode), 1, CASE WHEN CHARINDEX('-', RTRIM(FEDCode)) &gt; 0 THEN CHARINDEX('-', RTRIM(FEDCode))-1 ELSE LEN(RTRIM(FEDCode)) END) AS CHAR(8))) END  AS [Client Code] &#xA;,CASE WHEN FEDCode is null THEN RIGHT('00000000' + CONVERT(VARCHAR,fileno), 8) ELSE CAST(RIGHT(RTRIM(FEDCode),LEN(RTRIM(FEDCode))-CHARINDEX('-',RTRIM(FEDCode)))AS CHAR(8)) END  AS [Matter Number]&#xA;,clNo + '-'+ fileNo AS [3e References]&#xA;,dbFile.fileDesc AS [Matter Description]&#xA;,dbFile.Created AS [Date Opened]&#xA;,dbUser.usrFullName AS [Fee Earner]&#xA;,dbUser.usrAlias AS [Fee Earner Code]&#xA;,CASE WHEN client.ContactInfo IS NOT NULL THEN client.ContactInfo &#xA;&#x9;  WHEN hierarchylevel4hist IN ('Fraud and Credit Hire Liverpool','Motor Liverpool','Motor Management') THEN 'MotorBillDistribution@weightmans.com'&#xA;&#x9;  ELSE (CASE WHEN @test=1 THEN 'ExcludeTime@weightmans.com' ELSE  dbUser.usrEmail END)  END collate database_default AS [Fee Earner Email]&#xA;,InvMaster.InvNumber AS [Bill Number]&#xA;,InvMaster.InvDate AS [Bill Date]&#xA;,OrgAmt AS [Bill Amount]&#xA;,@Directory + LOWER(InvMasterID)+ '\' AS [Invoice Link]&#xA;,CASE WHEN bitMSOnlyMM=1 THEN 'MS' ELSE 'FED' END AS [SystemsDestination]&#xA;,CASE WHEN InvMaster.CrNoteNumber IS NOT NULL THEN 'Yes' ELSE 'No' END AS [Is Credit Note]&#xA;,InvMasterID&#xA;,IsReversed&#xA;,IsReversePayment&#xA;--,REPLACE(ISNULL(BaseUserName,SUBSTRING(coalesce(EmailAddr, (select distinct u1.emailaddr from TE_3E_Prod.dbo.NxWfHistoryHdr hdr&#xA;--inner join TE_3E_Prod.dbo.NxWfHistory h on hdr.HistHdrIndex = h.NxWfHistoryHdr&#xA;--inner join TE_3E_Prod.dbo.nxfwkuser u1 on u1.NxFWKUserID = hdr.Originator&#xA;--where h.JoinID = PM.ProfMasterID)),0, CHARINDEX('@',coalesce(EmailAddr, (select distinct u1.emailaddr from TE_3E_Prod.dbo.NxWfHistoryHdr hdr&#xA;--inner join TE_3E_Prod.dbo.NxWfHistory h on hdr.HistHdrIndex = h.NxWfHistoryHdr&#xA;--inner join TE_3E_Prod.dbo.nxfwkuser u1 on u1.NxFWKUserID = hdr.Originator&#xA;--where h.JoinID = PM.ProfMasterID))))),'.',' ') AS BaseUserName &#xA;,ISNULL(BaseUserName,REPLACE(REPLACE(CASE WHEN client.ContactInfo IS NOT NULL THEN client.ContactInfo &#xA;&#x9;  WHEN hierarchylevel4hist IN ('Fraud and Credit Hire Liverpool','Motor Liverpool','Motor Management') THEN 'MotorBillDistribution@weightmans.com'&#xA;&#x9;  ELSE (CASE WHEN @test=1 THEN 'Billing.ClaimantsPhotocopying@weightmans.com' ELSE  ISNULL(coalesce(EmailAddr, (select DISTINCT TOP 1 u1.emailaddr from TE_3E_Prod.dbo.NxWfHistoryHdr hdr&#xA;inner join TE_3E_Prod.dbo.NxWfHistory h on hdr.HistHdrIndex = h.NxWfHistoryHdr&#xA;inner join TE_3E_Prod.dbo.nxfwkuser u1 on u1.NxFWKUserID = hdr.Originator&#xA;where h.JoinID = PM.ProfMasterID))&#xA;,'Helen.Duffy@weightmans.com') END) END,'@weightmans.com',''),'.',' ')) AS BaseUserName&#xA;,CASE WHEN client.ContactInfo IS NOT NULL THEN client.ContactInfo &#xA;&#x9;  WHEN hierarchylevel4hist IN ('Fraud and Credit Hire Liverpool','Motor Liverpool','Motor Management') THEN 'MotorBillDistribution@weightmans.com'&#xA;&#x9;  ELSE (CASE WHEN @test=1 THEN 'Billing.ClaimantsPhotocopying@weightmans.com' ELSE  ISNULL(coalesce(EmailAddr, (select DISTINCT TOP 1 u1.emailaddr from TE_3E_Prod.dbo.NxWfHistoryHdr hdr&#xA;inner join TE_3E_Prod.dbo.NxWfHistory h on hdr.HistHdrIndex = h.NxWfHistoryHdr&#xA;inner join TE_3E_Prod.dbo.nxfwkuser u1 on u1.NxFWKUserID = hdr.Originator&#xA;where h.JoinID = PM.ProfMasterID))&#xA;,'Helen.Duffy@weightmans.com') END)  END AS EmailAddr&#xA;&#x9; &#xA;,PM.InternalComments AS [ProformaComments]&#xA;,Matter.BillingInstruc AS [BillingInstructions]&#xA;,CASE WHEN client.ContactInfo IS NOT NULL THEN 'Exception'&#xA;&#x9;  WHEN hierarchylevel4hist IN ('Fraud and Credit Hire Liverpool','Motor Liverpool','Motor Management') THEN 'Motor' ELSE 'FE' END AS [EmailType]&#xA;FROM TE_3E_PROD.dbo.InvMaster&#xA;INNER JOIN TE_3E_PROD.dbo.Matter&#xA; ON InvMaster.LeadMatter=Matter.MattIndex&#xA;INNER JOIN TE_3E_PROD.dbo.Client ON Matter.Client=Client.ClientIndex&#xA;INNER JOIN TE_3E_PROD.dbo.ProfMaster AS PM&#xA; ON InvMaster.LeadMatter=PM.LeadMatter&#xA; AND InvMaster.InvIndex=PM.InvMaster&#xA;LEFT JOIN TE_3E_PROD.dbo.NxFWKProcessItem AS Process WITH (NOLOCK) ON PM.OrigProcItemID = Process.ProcItemID&#xA;LEFT JOIN TE_3E_PROD.dbo.NxBaseUser WITH (NOLOCK) ON Process.UserId = NxBaseUserID&#xA;LEFT JOIN TE_3E_PROD.dbo.NxFWKUser WITH (NOLOCK) ON NxBaseUserID = NxFWKUser.NxFWKUserID&#xA;INNER JOIN MS_PROD.config.dbFile ON InvMaster.LeadMatter=dbFile.fileExtLinkID&#xA;INNER JOIN MS_PROD.config.dbClient ON dbFile.clID=dbClient.clID&#xA;INNER JOIN MS_PROD.dbo.udExtFile ON dbFile.fileID=udExtFile.fileID&#xA;INNER JOIN MS_PROD.dbo.dbUser ON dbFile.filePrincipleID=dbUser.usrID&#xA;LEFT OUTER JOIN [red_dw].[dbo].[dim_fed_hierarchy_history] ON usrAlias=fed_code collate database_default AND dss_current_flag='Y'&#xA;&#xA;WHERE InvMaster.InvNumber &lt;&gt;'PURGE'&#xA;AND IsReversed=0&#xA;AND CONVERT(DATE,InvMaster.InvDate,103)=CONVERT(DATE,GETDATE()-1,103)&#xA;&#xA;) AS AutoBills&#xA;LEFT OUTER JOIN TE_3E_PROD.dbo.NxAttachment AS DocName&#xA; ON InvMasterID=DocName.ParentItemID&#xA;ORDER BY [Bill Date],fileID,[Bill Number]" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
