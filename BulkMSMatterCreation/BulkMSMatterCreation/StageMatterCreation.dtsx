BCMUser.usrAlias AS TM,
td.Section,
s.SectionGroup,
td.Department,
dbBranch.brCode AS Office
from dbFeeEarner fe
INNER JOIN MS_PROD.dbo.dbUser AS MatterManager
 ON fe.feeusrID = MatterManager.usrID
INNER JOIN dbo.Syn3ETimeKeeper  t on fe.feeExtID = t.tkprindex
Inner Join dbo.Syn3ETkprDate  TD	on fe.feeExtID = TD.timekeeperlkup
