                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT AllData.SourceSystemID,
       AllData.dtePosted,
       CAST(AllData.txtItemCode AS NVARCHAR(15)) AS txtItemCode,
       CAST(AllData.txtItemDesc AS NVARCHAR(200)) AS txtItemDesc,
       AllData.curOffice,
       AllData.curClient,
       CAST(AllData.txtPayorPaye AS NVARCHAR(100)) AS txtPayorPaye,
       CAST(AllData.cboCatDesc AS NVARCHAR(15)) AS cboCatDesc
FROM 
(SELECT SourceSystemID
      ,VFile_Streamlined.dbo.DebtLedger.PostedDate AS [dtePosted]
	  ,ItemCode AS [txtItemCode]
      ,ItemDescription AS [txtItemDesc]
      ,Amount AS [curOffice]
      ,0 AS [curClient]
      ,NULL AS [txtPayorPaye]
      ,CASE WHEN TransactionType='COST' THEN '2'
	  WHEN TransactionType='DISB' THEN '1'
	  WHEN TransactionType='INVO' THEN '3' END 	   AS [cboCatDesc]
	   ,ItemCode,ItemDescription,TransactionType
  FROM VFile_Streamlined.dbo.DebtLedger
  INNER JOIN VFile_Streamlined.dbo.VFToMSMatters
    ON SourceSystemID=mt_int_code
  WHERE DebtOrLedger='Debt'
  AND TransactionType IN ('COST','INVO','DISB')
  UNION All
   SELECT Ledger.SourceSystemID,
         Ledger.dtePosted,
         Ledger.txtItemCode,
         Ledger.txtItemDesc,
         Ledger.curOffice,
         Ledger.curClient,
         Ledger.txtPayorPaye,
         Ledger.cboCatDesc,
         Ledger.ItemCode,
         Ledger.ItemDescription,
         Ledger.TransactionType
 FROM 
  (SELECT SourceSystemID
      ,VFile_Streamlined.dbo.DebtLedger.PostedDate AS [dtePosted]
      ,ItemCode AS [txtItemCode]
	  ,ItemDescription AS [txtItemDesc]
      ,CASE WHEN TransactionType&lt;&gt;'CR' THEN Amount ELSE 0 END AS [curOffice]
      ,CASE WHEN TransactionType='CR' THEN Amount ELSE 0 END AS [curClient]
      ,NULL AS [txtPayorPaye]
      ,CASE 
	  WHEN TransactionType='CR' THEN '5'
	  WHEN (recoverable=1 OR ItemCode IN ('NRCA','LRBI','LRCH')) THEN '1'	
	  WHEN NOT (recoverable=1 OR ItemCode IN ('NRCA','LRBI','LRCH')) THEN '0'	
	  WHEN ItemCode='' THEN '0'  END  AS [cboCatDesc]
	  ,ItemCode,ItemDescription,TransactionType
  FROM VFile_Streamlined.dbo.DebtLedger
  INNER JOIN VFile_Streamlined.dbo.VFToMSMatters
    ON SourceSystemID=mt_int_code
LEFT OUTER JOIN VFile_Streamlined.dbo.trdef
 ON ItemCode=tr_code
  WHERE DebtOrLedger='Ledger'
  ) AS Ledger
WHERE Ledger.cboCatDesc  IN ('5','0') -- Payments And Unrecoverable 

