,CAST(LEFT(RTRIM(NULL),50) AS NVARCHAR(50))  AS [contFirstNames]
,CAST(LEFT(RTRIM(NULL),50) AS NVARCHAR(50))  AS [contSurname]
,GETDATE() AS [contCreated]
,NULL AS [contEmail]
,CAST(LEFT(RTRIM(NULL),30) AS NVARCHAR(30)) AS [contTelHome]
,CAST(LEFT(RTRIM(NULL),30) AS NVARCHAR(30)) AS [contTelWork]
,CAST(LEFT(RTRIM(NULL),30) AS NVARCHAR(30)) AS [contTelMob]
,CAST(LEFT(RTRIM(NULL),30) AS NVARCHAR(30)) AS [contFAX]
,'No' AS [IsClient]
,CAST('ORGANISATION' AS NVARCHAR(12)) AS contSubType
,CAST(LEFT(RTRIM(clname),50) AS NVARCHAR(50)) AS contAddressee
,id AS VFileEntityCode
,DebtorOffice.col2,DebtorOffice.col3,col4,DebtorOffice.col5,DebtorOffice.col6,DebtorOffice.col7
FROM (
SELECT id,LTRIM(clname) AS clname,LTRIM(col1) AS col1,LTRIM(col2) AS col2,LTRIM(col3) AS col3,LTRIM(col4) AS col4,LTRIM(col5) AS col5,LTRIM(col6) AS col6,LTRIM(col7) AS col7
,CASE WHEN col1 IS NOT NULL THEN 1 ELSE 0 END +
CASE WHEN col2 IS NOT NULL THEN 1 ELSE 0 END +
CASE WHEN col3 IS NOT NULL THEN 1 ELSE 0 END +
CASE WHEN col4 IS NOT NULL THEN 1 ELSE 0 END +
CASE WHEN col5 IS NOT NULL THEN 1 ELSE 0 END +
CASE WHEN col6 IS NOT NULL THEN 1 ELSE 0 END +
CASE WHEN col7 IS NOT NULL THEN 1 ELSE 0 END  AS [NoLines]
FROM  DebtorOfficeAddress


