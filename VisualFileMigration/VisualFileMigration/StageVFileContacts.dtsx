WHEN  AllData.col6 &lt;&gt; AllData.addPostCode THEN AllData.col6  
ELSE NULL END AS NVARCHAR(50))  AS [addLine5]

,[addPostCode]
,[addDXCode]
,[contType]
,[contSalut]
,[contTitle]
,[contFirstNames]
,[contSurname]
,[contCreated]
,[contEmail]
