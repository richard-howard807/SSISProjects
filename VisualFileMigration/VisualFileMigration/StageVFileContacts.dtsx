        <pipeline
          version="1">
          <components>
            <component
              refId="Package\DFT - StageVFContacts\DFT - StageVFContacts"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="DFT - StageVFContacts"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT CASE WHEN CAST(LEFT(RTRIM(REPLACE(LEFT(AllData.[Name], CHARINDEX(':', AllData.[Name]) ),':','')),50) AS NVARCHAR(50))='' THEN CAST(LEFT(RTRIM(AllData.Name),50) AS NVARCHAR(50)) ELSE CAST(LEFT(RTRIM(REPLACE(LEFT(AllData.[Name], CHARINDEX(':', AllData.[Name]) ),':','')),50) AS NVARCHAR(50)) END  AS [clName]
,CAST(LEFT(RTRIM(AllData.Address1),50) AS NVARCHAR(50)) AS [addLine1]
,CAST(LEFT(RTRIM(AllData.Address2),50) AS NVARCHAR(50))  AS [addLine2]
,CAST(LEFT(RTRIM(AllData.Address3),50) AS NVARCHAR(50))  AS [addLine3]
,CAST(LEFT(RTRIM(AllData.Address4),50) AS NVARCHAR(50))  AS [addLine4]
,CAST(NULL AS NVARCHAR(50)) AS [addLine5]
,CAST(LEFT(RTRIM(AllData.PostCode),15) AS NVARCHAR(15)) AS [addPostCode]
,NULL AS [addDXCode]
,CAST(CASE WHEN ISNULL(AllData.Title,'')&lt;&gt;'' THEN 'INDIVIDUAL' ELSE 'ORGANISATION'  END AS NVARCHAR(12))  AS [contType]
,CAST(LEFT(RTRIM(AllData.Name),50) AS NVARCHAR(50)) AS [contSalut]
,CAST(LEFT(RTRIM(CASE WHEN ISNULL(AllData.Title,'')&lt;&gt;'' THEN   RTRIM(AllData.Title) ELSE NULL END),10) AS NVARCHAR(10))  AS [contTitle]
,CAST(LEFT(RTRIM(CASE WHEN ISNULL(AllData.Title,'')&lt;&gt;'' THEN  RTRIM(AllData.Forename) ELSE NULL END),50) AS NVARCHAR(50))  AS [contFirstNames]
,CAST(LEFT(RTRIM(CASE WHEN ISNULL(AllData.Title,'')&lt;&gt;'' THEN   RTRIM(AllData.Surname) ELSE NULL END),50) AS NVARCHAR(50))  AS [contSurname]
,GETDATE() AS [contCreated]
,NULL AS [contEmail]
,CAST(LEFT(RTRIM(AllData.HomeTelephone),30) AS NVARCHAR(30)) AS [contTelHome]
,CAST(LEFT(RTRIM(AllData.WorkTelephone),30) AS NVARCHAR(30)) AS [contTelWork]
,CAST(LEFT(RTRIM(AllData.Mobile),30) AS NVARCHAR(30)) AS [contTelMob]
,CAST(LEFT(RTRIM(AllData.Fax),30) AS NVARCHAR(30)) AS [contFAX]
,'No' AS [IsClient]
,CAST(CASE WHEN ISNULL(AllData.Title,'')&lt;&gt;'' THEN 'INDIVIDUAL' ELSE 'ORGANISATION'  END AS NVARCHAR(12)) AS contSubType
,CAST(LEFT(RTRIM(name),50) AS NVARCHAR(50)) AS contAddressee
,Code AS VFileEntityCode
FROM 
				(SELECT  CAST(matdb.mt_int_code AS INT) AS mt_int_code ,
                        CAST(CASE WHEN to_subcode = ';DTOR;1;DEB;19'
                                  THEN 'Primary Debtor'
                                  WHEN to_subcode = ';DTOR;2;DEB;19'
                                  THEN 'Secondary Debtor'
                                  WHEN to_subcode = ';DTOR;3;DEB;19'
                                  THEN 'Third Debtor'
                                  WHEN to_subcode = ';ATTA;1;DEM;17'
                                  THEN 'Primary Debtor Employeer'
                                  WHEN to_subcode = ';ATTA;2;DEM;17'
                                  THEN 'Secondary Debtor Employeer'
                                  WHEN to_subcode = ';ATTA;3;DEM;17'
                                  THEN 'Third Debtor Employeer'
                                  ELSE from_to_name
                             END AS VARCHAR(50)) AS ContactType ,
                        CAST(RTRIM(LTRIM(Entities.name)) AS VARCHAR(200)) AS Name ,
                        CAST(DateOfBirth AS DATE) AS DateofBirth ,
                        CAST(a_title AS VARCHAR(20)) AS Title ,
                        CAST(a_name AS VARCHAR(100)) AS Surname ,
                        CAST(a_forename AS VARCHAR(50)) AS Forename ,
                        CAST(a_sex AS VARCHAR(1)) AS Sex ,
                        CAST(a_address1 AS VARCHAR(100)) AS Address1 ,
                        CAST(a_address2 AS VARCHAR(70)) AS Address2 ,
                        CAST(a_address3 AS VARCHAR(70)) AS Address3 ,
                        CAST(a_address4 AS VARCHAR(70)) AS Address4 ,
                        CAST(a_postcode AS VARCHAR(70)) AS PostCode ,
                        CAST(telephone1 AS VARCHAR(40)) AS Telephone1 ,
                        CAST(telephone2 AS VARCHAR(40)) AS Telephone2 ,
                        CAST(fax_number AS VARCHAR(40)) AS Fax ,
                        CAST(b_title AS VARCHAR(20)) AS Second_Title ,
                        CAST(b_name AS VARCHAR(100)) AS Second_Surname ,
                        CAST(b_forename AS VARCHAR(50)) AS Second_Forename ,
                        CAST(b_sex AS VARCHAR(1)) AS Second_Sex ,
                        CAST(b_address1 AS VARCHAR(100)) AS Second_Address1 ,
                        CAST(b_address2 AS VARCHAR(70)) AS Second_Address2 ,
                        CAST(b_address3 AS VARCHAR(70)) AS Second_Address3 ,
                        CAST(b_address4 AS VARCHAR(70)) AS Second_Address4 ,
                        CAST(b_postcode AS VARCHAR(70)) AS Second_PostCode ,
                        HomeTelephone ,
                        WorkTelephone ,
                        Contact.Mobile,
						Entities.Code
                FROM    dbo.matdb AS matdb WITH ( NOLOCK )
                        INNER JOIN dbo.link link WITH ( NOLOCK ) ON matdb.mt_int_code = link.to_owner_code
                        INNER JOIN dbo.Entities Entities WITH ( NOLOCK ) ON link.from_owner_code = Entities.Code
                        INNER  JOIN dbo.relation AS R ON link.relation_int_code = r.relation_int_code
                        LEFT OUTER JOIN ( SELECT    mt_int_code AS From_owner_code ,
                                                    [ud_field##1] AS HomeTelephone ,
                                                    [ud_field##2] AS WorkTelephone ,
                                                    [ud_field##3] AS Mobile
                                          FROM      dbo.[uddetail] WITH ( NOLOCK )
                                          WHERE     [uds_type] = 'CON'
                                        ) AS Contact ON Entities.Code = Contact.From_owner_code
                        LEFT OUTER JOIN ( SELECT    matdb.mt_int_code ,
                                                    CASE WHEN link.to_subcode = ';DTOR;1;DEB;17'
                                                         THEN ';DTOR;1;DEB;19'
                                                         WHEN link.to_subcode = ';DTOR;2;DEB;17'
                                                         THEN ';DTOR;2;DEB;19'
                                                         WHEN link.to_subcode = ';DTOR;3;DEB;17'
                                                         THEN ';DTOR;3;DEB;19'
                                                    END AS Debtorlink ,
                                                    DOB.DateOfBirth AS DateOfBirth
                                          FROM      dbo.matdb AS matdb WITH ( NOLOCK )
                                                    INNER JOIN dbo.link AS link
                                                    WITH ( NOLOCK )
                                                    INNER JOIN dbo.Entities AS Entities
                                                    WITH ( NOLOCK ) ON link.from_owner_code = Entities.Code ON matdb.mt_int_code = link.to_owner_code
                                                    LEFT OUTER JOIN ( SELECT
                                                              u.mt_int_code AS [owner_code] ,
                                                              CONVERT(DATE, dbo.VarcharToDate(u.ud_field##5), 103) AS DateOfBirth
                                                              FROM
                                                              dbo.uddetail u
                                                              WITH ( NOLOCK )
                                                              WHERE
                                                              u.uds_type = 'MIS'
                                                              ) DOB ON Entities.Code = DOB.owner_code
                                          WHERE     link.to_subcode IN (
                                                    ';DTOR;1;DEB;17',
                                                    ';DTOR;2;DEB;17',
                                                    'DTOR;3;DEB;17' )
                                        ) AS DateOfBirth ON matdb.mt_int_code = DateOfBirth.mt_int_code
                                                            AND RTRIM(to_subcode) = RTRIM(Debtorlink)
							) AS AllData
							INNER JOIN  dbo.VFToMSMatters
							 ON AllData.mt_int_code=SourceSystemID
	WHERE CAST(LEFT(RTRIM(AllData.Name),50) AS NVARCHAR(50)) NOT  IN (':','&lt;No Details Entered&gt;','*:','.','.:')
	AND AllData.ContactType IN 
	('3rd Party Interest','3rd Party Interest in','3rd Party Interest''s Contact'
,'Claimant','Claimant in','Creditor','Creditor in','Creditor''s Contact','Creditor''s Contact in','Debt Agency'
,'Primary Debtor','Primary Debtor Employeer','Secondary Debtor')
AND AllData.PostCode &lt;&gt;''
