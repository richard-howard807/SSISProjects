,T.Base_Currency_Amount
,T.Base_Revalued_Amount
,T.Euro_Amount
,T.Euro_Revalued_Amount
,T.Transaction_Status
,T.Status_Reason
,T.Batch_Number
,T.Sub_Number
,T.Folio
,T.Unique_Key
,T.Details
,T.Novated
,T.Triang_Element
,H.MovementMap
,T.Event_Number
,T.Event_Name
,T.Claim_Report_Flag
,T.Loss_Type

From TransactionList T

 Left Join History H
 On T.Folio = H.Folio and T.Triang_Element = H.MovementMap and T.Payment_Type = H.Amount_Detail_Code

 Left Join Ledger L
