  )

 ,Ledger As

 (

 Select * FROM [SVR-LIV-3PTY-01].[Wellington_Live].[dbo].[dqvwPaymentBatches]

 )


 ,History As

  (Select Folio
  ,Create_Date
  ,Amend_Date
  ,Case When Account_Code = 'Claim_Payment' Then 'Paid Claims'
  When Account_Code = 'Claim_Outstanding' Then 'OSLR Movement' 
  Else 'Ignore' End As MovementMap
  ,Amount_Detail_Code

  FROM [SVR-LIV-3PTY-01].[Wellington_Live].[dbo].[dvwTransaction_Detail] where [Transaction_Status] &lt;&gt; 'X' and Trans_Amount &lt;&gt; 0
  and Case When Account_Code = 'Claim_Payment' Then 'Paid Claims'
  When Account_Code = 'Claim_Outstanding' Then 'OSLR Movement' 
  Else 'Ignore' End &lt;&gt; 'Ignore'
  )





 Select 
 T.FINANCE_GROUP
,T.FINANCIAL_PERIOD
,T.Transaction_Date
,H.Amend_Date
,L.Claim_Ledger_Date
,Case When Financial_Status = 'Provisional' then null
When Financial_Status &lt;&gt; 'Provisional' and T.Triang_Element = 'Paid Claims' then ISNULL(L.Claim_Ledger_Date,H.Amend_Date)
When Financial_Status &lt;&gt; 'Provisional' and T.Triang_Element = 'OSLR Movement' then H.Amend_Date
