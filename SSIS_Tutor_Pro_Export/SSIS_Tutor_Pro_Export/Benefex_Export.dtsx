                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT load_cascade_employee.displayemployeeid AS [Employee ID]
, load_cascade_employee.knownas AS [First Name]
, load_cascade_employee.surname AS [Last Name]
, load_cascade_employee.workemail AS [Email]
, load_cascade_employee_jobs.jobtitle AS [Job Title]
, hierarchylevel3 AS [Department]
, hierarchylevel2 AS [Business Unit]
, 'United Kingdom' AS [Country]
, ISNULL(CONVERT(VARCHAR, load_cascade_employee.contservicedate, 101),'') AS [Start Date]
, ISNULL(CONVERT(VARCHAR, load_cascade_employee.leftdate, 101),'') AS [Leave Date]
, linemanager.displayemployeeid AS [Manager Employee ID]
, CASE WHEN load_cascade_employee_jobs.levelidud ='Equity Partner' THEN 'NoReward' ELSE 'Reward' END AS [User Type]
, load_cascade_employee.workemail AS [SSO Unique Identifier]

FROM red_dw.dbo.load_cascade_employee
INNER JOIN red_dw.dbo.load_cascade_employee_jobs ON load_cascade_employee.employeeid =  load_cascade_employee_jobs.employeeid AND sys_activejob = 1 
INNER JOIN red_dw.dbo.load_cascade_valid_hierarchy_x ON load_cascade_employee_jobs.hierarchynode =  load_cascade_valid_hierarchy_x.hierarchynode
LEFT OUTER JOIN red_dw.dbo.load_cascade_employee linemanager ON linemanager.employeeid = load_cascade_employee_jobs.worksforemployeeid

WHERE 
load_cascade_employee_jobs.jobtitle &lt;&gt; 'Non-Executive Director'
AND load_cascade_employee.surname NOT LIKE 'Auto%'
AND load_cascade_employee.displayemployeeid NOT IN ('5597','3483')
AND load_cascade_employee_jobs.jobtitle NOT LIKE '%Payroll%'
AND load_cascade_employee_jobs.levelidud NOT IN ('Self Employed Consultant','Non-voting A Member','Non-Voting A Member')
AND (load_cascade_employee.leftdate IS NULL 
	OR load_cascade_employee.leftdate &gt;= (SELECT MIN(dim_date.calendar_date) date 
										FROM red_dw.dbo.dim_date
										WHERE dim_date.current_cal_month='Previous'))

ORDER BY [Last Name], [First Name]</property>
